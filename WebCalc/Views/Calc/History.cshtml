@Model IEnumerable<History>
@{
    ViewBag.Title = "History";
}

<h2>History</h2>
<table>
    <tr><th>операция</th></tr>

@foreach(var oper in Model)
{
    <tr>     
        <td>@oper.X</td>
        <td>@oper.Y</td>
        <td>@oper.Operation</td>
        <td>@oper.Result</td>
        <td>@oper.CreationDate</td>
    </tr>
}
</table>
